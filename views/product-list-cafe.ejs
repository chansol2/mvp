<div class="row">
  <div class="col-md-12 text-center mb-8" data-aos="fade-up" data-aos-delay="200">
    <div class="left-sidebar shop-sidebar-wrap">
      <div class="sidebar-widget mt-40px">
        <div class="sidebar-widget-tag d-inline-block">
          <ul>
            <li class="medium-cate-tab"><a class="active" >테이블</a></li>
            <li class="medium-cate-tab"><a>잔</a></li>
            <li class="medium-cate-tab"><a>식기</a></li>
            <li class="medium-cate-tab"><a>식탁보/테이블 매트/트레이</a></li>
            <li class="medium-cate-tab"><a>가전제품</a></li>
          </ul>
        </div>
      </div>
    </div>
  </div>
  <div class="col-md-12 text-right mb-4">
    <a href="/product/10101" class="text-bold">홈카페 더보기 →</a>
  </div>

<!-- initial bed rcmd -->

  <% for(var i = 0; i < 8; i++){ %>
  <div class="col-lg-3 col-md-6 col-sm-6 col-xs-6 mb-6 pr-1 pl-2" data-aos="fade-up" data-aos-delay="200">
    <!-- Single Prodect -->
    <div class="product">
      <div class="thumb">
        <a href="/product/<%= spl_1[i].cate_id %>/detail?pid=<%= spl_1[i].id %>&o_url=<%= spl_1[i].org_url%>" class="image">
          <img src="<%= spl_1[i].img_url %>" alt="Product" />
        </a>
        <div class="actions">
          <a href="/" class="action wishlist" title="Wishlist"><i class="icon-heart"></i></a>
        </div>
      </div>
      <div class="content">
        <h5 class="title"><%= spl_1[i].seller_nm %></h5>
        <h5 class="name"><%= spl_1[i].prd_nm %></h5>
        <span class="price">
          <span class="new"><%= spl_1[i].sales_price %>원</span>
        </span>
      </div>
    </div>
  </div>
  <% } %>

</div>

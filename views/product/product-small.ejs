    <!-- Shop Category pages -->
    <div class="shop-category-area">
      <div class="container">
        <div class="row">
          <div class="col-lg-9 order-lg-last col-md-12 order-md-first pl-width">
            <!-- Shop Top Area Start -->
            <div class="d-block">
              <div class="custom-lnb">
                <p>침실가구 / 화장대</p>
              </div>
              <div class="d-flex sorting-alignment">
                <div class="d-block">
                  <h4 class="align-self-center">침대/침대 헤드</h4>
                  <div class="search-result">
                    <p>100개의 결과가 있습니다.</p>
                  </div>
                </div>
                <div class="select-shoing-wrap d-flex align-items-center sorting-width">
                  <div class="shop-select sorting-style">
                    <select class="shop-sort">
                      <option data-display="">인기순</option>
                      <option value="1">리뷰순</option>
                      <option value="2">평점순</option>
                      <option value="3">가격높은순</option>
                      <option value="4">가격낮은순</option>
                    </select>
                  </div>

                </div>
              </div>
            </div>
            <!-- Shop Top Area End -->

            <!-- Shop Bottom Area Start -->
            <div class="shop-bottom-area">
              <div class="row"><%include ./product-medium-list %></div>
              <!--  Pagination Area Start -->
              <div
                class="
                  pro-pagination-style
                  text-center
                  mb-md-30px mb-lm-30px
                  mt-6
                "
                data-aos="fade-up"
              >
                <ul>
                  <li>
                    <a class="prev" href="#"
                      ><i class="ion-ios-arrow-left"></i
                    ></a>
                  </li>
                  <li><a class="active" href="#">1</a></li>
                  <li><a href="#">2</a></li>
                  <li>
                    <a class="next" href="#"
                      ><i class="ion-ios-arrow-right"></i
                    ></a>
                  </li>
                </ul>
              </div>
              <!--  Pagination Area End -->
            </div>
            <!-- Shop Bottom Area End -->
          </div>
          <!-- here -->
